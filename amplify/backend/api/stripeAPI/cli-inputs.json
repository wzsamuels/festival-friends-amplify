{
  "version": 1,
  "paths": {
    "/checkout": {
      "name": "/checkout",
      "lambdaFunction": "stripeCheckout",
      "permissions": {
        "setting": "protected",
        "auth": [
          "create"
        ],
        "guest": [
          "create"
        ]
      }
    },
    "/create-payment-portal": {
      "name": "/create-payment-portal",
      "lambdaFunction": "createPaymentPortal",
      "permissions": {
        "setting": "private",
        "auth": [
          "create"
        ]
      }
    },
    "/test": {
      "name": "/test",
      "lambdaFunction": "test",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
    },
    "/cancel-subscription": {
      "name": "/cancel-subscription",
      "lambdaFunction": "cancelSubscription",
      "permissions": {
        "setting": "private",
        "auth": [
          "create"
        ]
      }
    }
  }
}